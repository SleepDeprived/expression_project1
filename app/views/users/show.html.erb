<h1>Welcome <%= "#{@user.first_name} #{@user.last_name}" %></h1>
<div>
  <%= form_tag("/results", method: :get) do %>
    <%= label_tag  "Search by gene name"%>
  <%= text_field_tag "gene", nil, placeholder: "gene name" %>
    <%= submit_tag "Search", name: "search by gene" %>
  <% end %>
</div>

<div>
  <%= form_tag("/results", method: :get) do %>
  <%= label_tag "Search by location of expression" %>
    <%= text_field_tag "location", nil, placeholder: "location" %>
  <%#= label_tag "time of expression" %>
    <%#= text_field_tag "time", nil, placeholder: "stage" %>
  <%= submit_tag "Search", name: "search by location" %>
  <% end %>
</div>


<h1>Stocks in My Collection:</h1>
<% @user.collections.each do |collection| %>
  <%= collection.gal4_line.name %>
<% end %>

<a href="/gal4_lines/new">Add a new Gal4 line to the database</a>


